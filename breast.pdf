<script type="text/javascript">

    var idx = 0;
    var alerts = [];

    var controllerUrl = '/login/GetAlerts/';
    console.log(controllerUrl);
    $(document).ready(
        function () {
            $.ajax({
                type: "GET",
                url: controllerUrl,
                data: param = "",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data, status) {
                    console.log(data);
                    alerts = data;
                    if (alerts.length > 0) {
                        if (idx >= alerts.length) idx = 0;
                        var alert = alerts[idx];
                        $("#alert").text(alert.Title);
                        $("#alert").mouseenter(function(){
                            $(this).attr('title', alert.Description);
                        });
                        $("#alert").click(function () {
                            location.href = alert.Link;
                        });
                        setInterval(function () {
                            idx++;
                            if (idx >= alerts.length) idx = 0;
                            alert = alerts[idx];
                            $("#alert").fadeOut(300, function () {
                                var alert = alerts[idx];
                                $("#alert").text(alert.Title);
                                $("#alert").mouseenter(function(){
                                    $(this).attr('title', alert.Description);
                                });
                                $("#alert").click(function () {
                                    location.href = alert.Link;
                                });
                                $("#alert").fadeIn(300);
                            });
                        }, 8000);
                    }

                },
                error: function () {

                }
            });


            $("#addToFavorites").click(function () {
                var pageName = $("#pageName").length > 0 ? $("#pageName").val() : "";
                var favoritePage = { QueryString: 'ReturnURL%3dhttps%253a%252f%252fwww.nccn.org%252fprofessionals%252fphysician_gls%252fpdf%252fbreast.pdf', PageName: pageName}; 
                $.ajax({
                    type: "POST",
                    url: '/login/AddOrRemovePageToFavorites/',
                    data: JSON.stringify(favoritePage),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    beforeSend: function () {
                        $('.common-loading').css('display', 'block');
                    },
                    success: function (data, status) {
                        $('.common-loading').css('display', 'none');
                        console.log("success");

                        $addRemoveBtn = $("#addToFavorites");
                        if ($addRemoveBtn.hasClass("in-favorites")) {
                            console.log("remove from favorites");
                            $addRemoveBtn.removeClass("in-favorites");
                        }
                        else {
                            console.log("add to favorites");
                            $addRemoveBtn.addClass("in-favorites");
                        }
                    },
                    error: function (er) {
                        $('.common-loading').css('display', 'none');
                        console.log("error");
                        console.log(er);
                    }
                });





            });



        }
    );

</script>
