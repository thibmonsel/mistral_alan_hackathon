<script type="text/javascript">
    var responsive = false;

    $(function () {
        responsive = window.innerWidth < 992; 
        $(".dropdown-menu [data-toggle='dropdown']").parents(".dropdown-menu").on('shown.bs.dropdown', function (event) {
            $(".level-3").removeClass("show");

            var parentNodeId = $(this).attr("aria-labelledby");
            $("#" + parentNodeId).addClass("active");
            //$("#" + parentNodeId).bind("click", function () {  currentNode.removeClass("show") });
        });

        $(".dropdown-menu [data-toggle='dropdown']").parents(".dropdown-menu").on('hidden.bs.dropdown', function (event) {
            var parentNodeId = $(this).attr("aria-labelledby");
            $("#" + parentNodeId).removeClass("active");
        });


        $(".dropdown-menu [data-toggle='dropdown']").on("click", function (event) {
            if (responsive) {
                event.preventDefault(); //comment to  proccess click
                }
            event.stopPropagation();

            var target = $(this).data("target");
            $(this).parent().siblings().removeClass("show");
            $(this).parent().siblings(target).addClass("show");

        });
    });

    $(window).resize(function () {
        responsive = window.innerWidth < 992;
    });

    // show-hide menu on hover
//    $('.nav-link.dropdown-toggle.top-link').hover( //only top menu shows on hover

    $('.dropdown-toggle').hover(
        function () {
            if (!responsive) {
                var targetId = $(this).attr("data-target");
                    $(targetId).stop(true, true).delay(500).fadeIn();
                }
        },
        function () {
            if (!responsive) {
            var targetId = $(this).attr("data-target");
                $(targetId).stop(true, true).delay(100).fadeOut();
                }
        }
    );

    $('#nccnNavbar .dropdown-menu').hover(
        function () {
            if (!responsive) {
                $(this).stop(true, true);
                }
        },
        function () {
            if (!responsive) {
                $(this).stop(true, true).hide().delay(100).fadeOut();
                }
        }
    );

</script>

