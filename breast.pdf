<script type="text/javascript">
    $.ajax({
        type: "GET",
        data: param = "",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        url: "/home/store/order-history/GetOrderList",
        success: function (data, status) {
            $('#orderListBlock').empty();
            $.each(data.Orders, function (index, order) {
                $('#orderListBlock').append("<a class='link-text' href='/home/store/order-history'>" + order.Name + "</a>");
            });
            if (data.ShowMore) {
                $('#orderListBlock').append("<a class='show-more' href='/home/store/order-history'>View More</a>");
            }
        },
        error: function (request, status, error) {
            //     alert(request.responseText);
        }
    });

    $.ajax({
        type: "GET",
        data: param = "",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        url: "/home/store/subscriptions/GetSubscriptionList",
        success: function (data, status) {
            $('#subscriptionListBlock').empty();
            $.each(data.Subscriptions, function (index, subscription) {
                $('#subscriptionListBlock').append("<a class='link-text' href='/home/store/subscriptions'>" + subscription.ParentProduct + "</a>");
            });
            if (data.ShowMore) {
                $('#subscriptionListBlock').append("<a class='show-more' href='/home/store/subscriptions'>View More</a>");
            }
        },
        error: function (request, status, error) {
            //   alert(request.responseText);
        }
    });

    let url = "/login/GetMenuLinkList";
    const idList = ['#favoritepagesListBlock', '#meetingsanddisclosuresBlock', '#memberinstitutionBlock']

    $.ajax(url, {
        type: "GET",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        success: function (data, status) {
            for (id of idList) $(id).empty();
            for (item of data) {
                $(idList[item.PageSection - 1]).append(`<a class='link-text' href='${item.PageURL}'>${
                    item.PageSection == 3 ?
                        `<b>${item.PageName}</b>` :
                        item.PageName
                    }</a>`);
            }
        },
        error: function (request, status, error) {
            //   alert(request.responseText);
        }
    });

</script>
